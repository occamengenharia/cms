{
  "kind": "collectionType",
  "collectionName": "histories",
  "info": {
    "name": "history"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "year": {
      "type": "integer",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "photo": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    }
  }
}
